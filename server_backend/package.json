{
  "name": "asabik-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start:dev": "nest start --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prettier": "npx prettier --write . ",
    "prepare": "husky install",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d src/data-source.ts migration:generate src/migration/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create src/migration/$npm_config_name"
  },
  "dependencies": {
    "@anchan828/nest-sendgrid": "^0.6.33",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/passport-local": "^1.0.34",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "express-session": "^1.17.2",
    "file-type": "^17.1.1",
    "handlebars": "^4.7.7",
    "js-statistics": "^0.0.6",
    "jsonwebtoken": "^9.0.0",
    "jstat": "^1.9.6",
    "mathjs": "^11.8.1",
    "mysql2": "^2.3.3",
    "nestjs-typeorm-paginate": "^4.0.3",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.1",
    "plaid": "^12.1.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "stats-lite": "^2.2.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.5.2",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "",
    "testRegex": ".*\\.(e2e-)?spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
